{
  "$id": "https://open-metadata.org/schema/api/lineage/esLineageData.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "EsLineageData",
  "description": "Response object for the search lineage request from Elastic Search.",
