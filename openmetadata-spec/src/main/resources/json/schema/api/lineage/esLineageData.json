    },
    "columns": {
      "description": "Columns associated.",
      "type": "array",
      "items": {
        "$ref": "../../type/entityLineage.json#/definitions/columnLineage"
      }
    },
    "description": {
      "description": "Description.",
      "type": "string"
    },
    "source": {
      "description": "Source of the Lineage.",
      "type": "string"
    },
    "doc_id": {
      "description": "Doc Id for the Lineage.",
      "type": "string"
    },
